		"migration": "knex migrate:latest",
		"rollback": "knex migrate:rollback --all"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.2",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"jsonwebtoken": "^9.0.0",
		"knex": "^2.4.2",
		"moment-timezone": "^0.5.41",
		"multer": "1.4.5-lts.1",
		"pg": "^8.9.0",
		"pm2": "^5.2.2",
		"sqlite3": "^5.1.4"
	},
	"devDependencies": {
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"nodemon": "^2.0.20",
		"prettier": "^2.8.4",
		"rewire": "^6.0.0"
	}
